
@{
    ViewBag.Title = "ListeClients";
}

    <body>
        <table>
            <tr>
                <th>Nom</th>
                <th>Age</th>
            </tr>
            @foreach (firstApp.Models.Client client in ViewData["Clients"] as List<firstApp.Models.Client>)
            {
                <tr>
                    <td>@client.Nom</td>
                    <td>@client.Age</td>
                </tr>
            }
        </table>
    </body>

